{
    "repositories": [
        { "type": "vcs", "url": "https://github.com/undkonsorten/registeraddress.git" }

    ],
    "name": "undkonsorten/registeraddress-logger",
    "type": "typo3-cms-extension",
    "description": "Add logging to registeraddress to full fill law needs  ",
    "authors": [
        {
            "name": "Eike Starkmann",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^12.4 || ^13",
        "afm/registeraddress": "^5 || ^6"
    },
    "autoload": {
        "psr-4": {
            "Undkonsorten\\RegisteraddressLogger\\": "Classes"
        }
    },
	"extra": {
		"typo3/cms":{
			"extension-key": "registeraddress_logger"
		},
        "branch-alias": {
            "dev-main": "1.4.x-dev"
        }
	}
}
